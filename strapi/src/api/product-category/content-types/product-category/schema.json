{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "singularName": "product-category",
    "pluralName": "product-categories",
    "displayName": "Product Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "default": "Category Name",
      "required": true,
      "maxLength": 50,
      "minLength": 3
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "product": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "product_category"
    }
  }
}
